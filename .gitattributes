# Auto detect text files and perform LF normalization
* text=auto
.dll filter=lfs diff=lfs merge=lfs -text
.pid filter=lfs diff=lfs merge=lfs -text
.bin filter=lfs diff=lfs merge=lfs -text
.meta filter=lfs diff=lfs merge=lfs -text
.pref filter=lfs diff=lfs merge=lfs -text
.md filter=lfs diff=lfs merge=lfs -text
.git filter=lfs diff=lfs merge=lfs -text
lfs filter=lfs diff=lfs merge=lfs -text
track filter=lfs diff=lfs merge=lfs -text
*.psd filter=lfs diff=lfs merge=lfs -text
*.md filter=lfs diff=lfs merge=lfs -text
*.dll filter=lfs diff=lfs merge=lfs -text
*.asset filter=lfs diff=lfs merge=lfs -text
*.pref filter=lfs diff=lfs merge=lfs -text
*.sref filter=lfs diff=lfs merge=lfs -text
